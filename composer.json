{
    "name": "skyraptor/laravel-achievements",
    "description": "Achievement-System for Laravel",
    "keywords": ["laravel", "achievements"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Simonetti",
            "email": "simonettigo@gmail.com"
        },
        {
            "name": "Patrick Henninger",
            "email": "privat@skyraptor.eu"
        }
    ],
    "autoload": {
        "classmap": [
            "src/Migrations"
        ],
        "psr-4": {
            "SkyRaptor\\Achievements\\": "src/"            
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SkyRaptor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SkyRaptor\\Achievements\\AchievementsServiceProvider"
            ]
        }
    },
    "require": {
        "ramsey/uuid": "^3.7|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "laravel/laravel": "^6.0|^7.0"
    }
}
